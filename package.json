{
  "name": "coc-myplugin",
  "version": "0.1.20",
  "description": "My awesome coc.nvim plugin",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "postinstall": "npm run build && cd out && npm install --production"
  },
  "engines": {
    "coc": "^0.0.82"
  },
  "keywords": [
    "coc.nvim",
    "vim",
    "neovim"
  ],
  "files": [
    "lib",
    "README.md",
    "package.json"
  ],
  "author": "votre_nom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nekocheik/my-avante.git"
  },
  "activationEvents": [
    "*"
  ],
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "coc.nvim": "^0.0.83-next.9",
    "esbuild": "^0.24.2",
    "esbuild-node-externals": "^1.16.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "myplugin.helloWorld",
        "title": "Say Hello World"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "MyPlugin Configuration",
      "properties": {
        "myplugin.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable my plugin"
        },
        "myplugin.mySetting": {
          "type": "string",
          "default": "defaultValue",
          "description": "Some setting for my plugin"
        }
      }
    }
  }
}
