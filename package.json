{
  "name": "coc-myplugin",
  "version": "0.1.8",
  "description": "My awesome coc.nvim plugin",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "engines": {
    "coc": "^0.0.82"
  },
  "keywords": [
    "coc.nvim",
    "vim",
    "neovim"
  ],
  "files": [
    "lib",
    "README.md",
    "package.json"
  ],
  "author": "votre_nom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nekocheik/my-avante.git"
  },
  "activationEvents": [
    "*"
  ],
  "devDependencies": {
    "coc.nvim": "^0.0.83-next.9"
  },
  "contributes": {
    "commands": [
      {
        "command": "myplugin.helloWorld",
        "title": "Say Hello World"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "MyPlugin Configuration",
      "properties": {
        "myplugin.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable my plugin"
        },
        "myplugin.mySetting": {
          "type": "string",
          "default": "defaultValue",
          "description": "Some setting for my plugin"
        }
      }
    }
  },
  "dependencies": {
    "dotenv": "^16.4.7"
  }
}
